.container {
  display: flex;
  flex-direction: column;
  background-color: #fff;
  margin: 0;
  padding: 0;
  width: 100%;
  padding-top: 96rpx;
  box-sizing: border-box;
}

.tab{
  position: fixed;
  width: 100%;
  /*悬停关键样式*/
  top: 0;
  left: 0;
  z-index: 999;
  background-color: #fff;
}

.search_container{
  display: flex;
  flex-direction: row;
  justify-content: flex-start;
  align-items: center;
  background-color: #F9FAFBFF;
  border-radius:38rpx;
  height: 76rpx;
  box-sizing: border-box;
  padding: 20rpx 32rpx;
  margin: 22rpx 32rpx;
}

.letf_search{
  width: 24rpx;
  height: 24rpx;
  flex-shrink: 0;
  margin-right: 20rpx;
}

.text{
  font-size: 24rpx;
  color: #999999FF;
}





page {
  width: 100%;
  height: 100%;
  background-color: #fff;
}
